{
    "name": "jcchavezs/zipkin-php",
    "description": "A zipkin instrumentation for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "José Carlos Chávez",
            "email": "jcchavezs@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^5.6||^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7.19",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Zipkin\\": "./src/Zipkin/"
        },
        "files": ["./src/Zipkin/Timestamp.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "ZipkinTests\\": "./tests/"
        }
    }
}
